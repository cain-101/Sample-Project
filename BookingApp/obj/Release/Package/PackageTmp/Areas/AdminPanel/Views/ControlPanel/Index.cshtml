            @if (ViewBag.OTDList != null)
            {
                <fieldset class="scheduler-border p-2">
                    <legend class="scheduler-border text-dark">
                        <i style='font-size:30px;border-radius:3px' class="bg-info p-3 fas fa-plane-departure text-white"></i>
                        <strong>Out To Date Tour Packages:</strong>
                    </legend>

                    <div class="row">
                        @if (Enumerable.Count(ViewBag.OTDList) < 1)
                        {
                            <h4 class="p-5"> No out to date packages! </h4>
                        }
                        @foreach (var item in ViewBag.OTDList)
                        {
                            <div class="col-md-4">
                                <div class="card card-product">
                                    <div class="card-header card-header-image" data-header-animation="true">
                                        <a href="#pablo">
                                            @if (item.p_img == null)
                                            {
                                                <img class="img" height="120" src="~/DynamicFiles/defaultheader.jpg" />
                                            }
                                            else
                                            {
                                                <img class="img" src="~/DynamicFiles/@item.p_img" />
                                            }
                                        </a>
                                    </div>
                                    <div class="card-body">
                                        <div class="card-actions text-center">
                                            <button type="button" class="btn btn-danger btn-link fix-broken-card">
                                                <i class="material-icons">build</i> Fix Header!
                                            </button>
                                            <a href="@Url.Action("ViewOTDP", "ControlPanel", new { @id = item.p_id})" class="btn btn-default btn-link" rel="tooltip" data-placement="bottom" title="View">
                                                <i class="material-icons">art_track</i>
                                            </a>
                                            @*<a href="@Url.Action("", "ViewOTDP", new { @id = item.p_id})" class="btn btn-success btn-link" rel="tooltip" data-placement="bottom" title="Edit">
                                                    <i class="material-icons">edit</i>
                                                </a>
                                                <a href="@Url.Action("ControlPanel", "ViewOTDP", new { @id = item.p_id})" class="btn btn-danger btn-link" rel="tooltip" data-placement="bottom" title="Remove">
                                                    <i class="material-icons">close</i>
                                                </a>*@
                                        </div>
                                        <h4 class="card-title">
                                            <a href="#">@item.p_name</a>
                                        </h4>
                                        @*<div class="card-description">

                                            </div>*@
                                    </div>
                                    <div class="card-footer row">
                                        @for (int d = 0; d < item._Dates.Count; d++)
                                        {
                                            <div class="card-header-icon text-center col-12 m-0 p-0">
                                                <div class="stats">
                                                    <i class="material-icons" style="color:#e34627">date_range</i> <del style="color:red">@item._Dates[d].otDates</del>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </fieldset>
            }
        </div>
    </div>
</div>


<!-- The Modal EditTP -->
<div class="modal fade" id="EditTP">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <h4 class="modal-title">Tour Package Updating</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
                <form method="post" enctype="multipart/form-data">
                    <div class="row" name="updateTPDiv">
                        <span id="pId">

                        </span>
                        <div class="col-12 col-sm-6 col-md-6 col-lg-6 col-xl-6 mb-1" id="pName">

                        </div>
                        <div class="col-12 col-sm-6 col-md-6 col-lg-6 col-xl-6 mb-1" id="pPrice">

                        </div>
                        <div class="col-12 col-sm-6 col-md-6 col-lg-6 col-xl-6">
                            <select class="custom-select dd-user my-font-13" name="cType" id="cType">
                                @* Dynamic option populating *@
                            </select>
                        </div>
                        <div class="col-12 col-sm-6 col-md-6 col-lg-6 col-xl-6">
                            <select class="custom-select dd-user my-font-13" name="bType" id="bType">
                                @* Dynamic option populating *@
                            </select>
                        </div>
                        <div class="col-12 col-sm-6 col-md-6 col-lg-6 col-xl-6 mt-2">
                            <select class="custom-select dd-user my-font-13" name="pStatus" id="pStatus">
                                @* Dynamic option populating *@
                            </select>
                        </div>
                        <div class="col-12 mt-1">
                            <button id="updateTP" class="btn btn-success pull-right">Update</button>
                        </div>
                    </div>
                </form>
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>

        </div>
    </div>
</div>